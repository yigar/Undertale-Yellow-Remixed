{
    "dialogueGroups":
    [
        {
            "name": "Martlet Wakes Up",
            "parameters": {
                "defaultDialogue": true,
                "checkCount": 0
            },
            "dialogue":
            [
                {
                    "character": "Martlet",
                    "emotion": "shocked",
                    "string": "Hm? Oh, uh, hello!"
                },
                {
                    "character": "Martlet",
                    "emotion": "nervousSweat",
                    "string": "Sorry, I was zoned out for a moment there."
                },
                {
                    "character": "Martlet",
                    "emotion": "smug",
                    "string": "You look like you're lost. Do you need directions?"
                },
                {
                    "character": "Martlet",
                    "emotion": "nervousSmile",
                    "string": "I'm no information kiosk, though it kind of looks like it..."
                },
                {
                    "character": "Martlet",
                    "emotion": "happySquint",
                    "string": "But I know this place like the back of my wing!"
                },
                {
                    "character": "Martlet",
                    "emotion": "cheery",
                    "string": "I can get you wherever you need to go!"
                },
                {
                    "character": "Martlet",
                    "emotion": "sneer",
                    "string": "Wait..."
                },
                {
                    "character": "Martlet",
                    "emotion": "sideSneer",
                    "string": "I'm gonna take a shot in the dark here, but..."
                },
                {
                    "character": "Martlet",
                    "emotion": "determined",
                    "string": "You're a human! Right?"
                },
                {
                    "character": "Martlet",
                    "emotion": "determined",
                    "string": "I knew it! I knew I'd find one! You must be human! Right? Right?"
                },
                {
                    "character": "Martlet",
                    "emotion": "cheery",
                    "string": "Your hat! It's like in those human movies!"
                },
                {
                    "character": "Martlet",
                    "emotion": "smile",
                    "string": "They used to have a bunch of them over at the library."
                },
                {
                    "character": "Martlet",
                    "emotion": "sideConfused",
                    "string": "I'm not sure what happened to them..."
                },
                {
                    "character": "Martlet",
                    "emotion": "shocked",
                    "string": "Ah! I'm getting off-track again."
                }
            ],
            "callback": {
                "script": "martletMeetsPlayer",
                "func": "martletWalkOver",
                "args": []
            }
        },
        {
            "name": "Martlet Book",
            "parameters": {
                "defaultDialogue": false,
                "checkCount": 0
            },
            "dialogue":
            [
                {
                    "character": "Martlet",
                    "emotion": "determined",
                    "string": "Time for some Royal Guard protocol!"
                },
                {
                    "character": "Martlet",
                    "emotion": "determined",
                    "string": "I hope you're ready, 'cause I memorized it!"
                }
            ],
            "callback": {
                "script": "martletMeetsPlayer",
